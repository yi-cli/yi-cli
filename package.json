{
  "name": "yi-cli",
  "version": "0.0.1",
  "description": "A simple CLI for scaffolding projects.",
  "preferGlobal": true,
  "bin": {
    "yi": "bin/yi",
    "yi-init": "bin/yi-init",
    "vue-list": "bin/vue-list"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint",
    "precommit": "lint-staged",
    "lint": "eslint --fix ./lib/**/*.js",
    "postinstall": "node ./lib/doctor",
    "postupdate": "node ./lib/doctor"
  },
  "lint-staged": {
    "lib/**/*.js": ["eslint --fix", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yi-cli/yi-cli.git"
  },
  "keywords": [
    "yi",
    "cli",
    "es6"
  ],
  "author": "yuyi",
  "license": "MIT",
  "dependencies": {
    "async": "^2.5.0",
    "bin-version-check": "^3.0.0",
    "chalk": "^2.0.1",
    "commander": "^2.11.0",
    "consolidate": "^0.15.0",
    "download-git-repo": "^1.0.1",
    "each-async": "^1.1.1",
    "figlet": "^1.2.0",
    "handlebars": "^4.0.11",
    "inquirer": "^3.2.0",
    "metalsmith": "^2.3.0",
    "minimatch": "^3.0.4",
    "multimatch": "^2.1.0",
    "node-symbols": "0.0.4",
    "ora": "^1.3.0",
    "read-metadata": "^1.0.0",
    "request": "^2.83.0",
    "rimraf": "^2.6.1",
    "semver": "^5.4.1",
    "user-home": "^2.0.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "eslint-config-yi": "^0.0.4"
  }
}
